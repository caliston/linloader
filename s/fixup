	AREA	FIXUP, CODE, READONLY

	EXPORT	fixup_start
	EXPORT	fixup_end

fixup_start
	SUB	r3, r3, #1
	ADR	r4, fixup_end
loop1	LDMIA	r4!, {r5}
loop2	LDMIA	r5!, {r6, r7}
	TEQ	r6, #0
	BEQ	start_kernel
loop3	LDMIA	r6!, {r0, r8, r9, r10, r11, r12, r13, r14}
	STMIA	r7!, {r0, r8, r9, r10, r11, r12, r13, r14}
	LDMIA	r6!, {r0, r8, r9, r10, r11, r12, r13, r14}
	STMIA	r7!, {r0, r8, r9, r10, r11, r12, r13, r14}
	TST	r6, r3
	BNE	loop3
	TST	r5, r3
	BNE	loop2
	B	loop1

start_kernel
	MOV	r0, #0
	MOV	pc, r2

	LTORG
fixup_end

	END
